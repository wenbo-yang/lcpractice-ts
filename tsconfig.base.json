{
    "compilerOptions": {
        "alwaysStrict": true,
        "declaration": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist",
        "target": "es2022",
        "lib": ["es2022"],
        "incremental": true,
        "tsBuildInfoFile": "./.buildcache",
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "esModuleInterop": true
    }
}
